                android:layout_height="150dp"
                android:layout_marginHorizontal="@dimen/_5sdp"
                android:background="@drawable/white_shape_edit"
                android:backgroundTint="@color/gray" />


            <View
                android:layout_width="@dimen/_75sdp"
                android:layout_height="150dp"
